<%= raw calendar(:year => Time.now.year, :month => Time.now.month) { |day| 
  booked_date = Date.civil(2011, 07, 24)
  res = Array.new
  if day.eql?(booked_date)
   puts day
   cell_text = day.day.to_s
   cell_attrs = Hash.new
   cell_attrs[:class] = "booked"
  end
  res.push(cell_text)
  res.push(cell_attrs)
  res
 }
%>